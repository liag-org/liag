{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch --minify",
    "dev:remix": "remix dev",
    "start": "remix-serve build",
    "generate:css": "npx tailwindcss -i ./tailwindcss/global.css -o ./app/glossy/app.css",
    "typecheck": "tsc -b",
    "i18n:pull": "i18nexus pull",
    "i18n:push": "i18nexus push"
  },
  "dependencies": {
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "framer-motion": "^9.0.2",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.4",
    "remix-i18next": "^5.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.11.1",
    "@remix-run/eslint-config": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@remix-run/server-runtime": "^1.11.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "i18nexus-cli": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
